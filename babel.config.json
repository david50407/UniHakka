{
  "env": {
    "production": {
      "presets": [
        "@babel/preset-typescript",
        "@babel/preset-env"
      ],
      "plugins": [
        "@babel/proposal-class-properties",
        [
          "module-resolver",
          { "root": ["."], "alias": { "@": "." } }
        ]
      ]
    },
    "development": {
      "presets": [
        "@babel/preset-typescript",
        "@babel/preset-env"
      ],
      "plugins": [
        "@babel/proposal-class-properties",
        [
          "module-resolver",
          { "root": ["."], "alias": { "@": "." } }
        ]
      ]
    }
  }
}